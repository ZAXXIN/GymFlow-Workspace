{
  "name": "gymflow-web",
  "version": "1.0.0",
  "description": "GymFlow 健身工作室管理系统 - Web管理后台",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "type-check": "vue-tsc --noEmit",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "dayjs": "^1.11.10",
    "echarts": "^5.4.3",
    "element-plus": "^2.4.1",
    "file-saver": "^2.0.5",
    "lodash-es": "^4.17.21",
    "pinia": "^2.1.7",
    "qrcode": "^1.5.3",
    "uuid": "^9.0.1",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@types/file-saver": "^2.0.5",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.10.5",
    "@types/qrcode": "^1.5.2",
    "@types/uuid": "^9.0.7",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.0",
    "eslint": "^8.55.0",
    "eslint-plugin-vue": "^9.19.2",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.0",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.0",
    "vue-tsc": "^1.8.22"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "keywords": [
    "gymflow",
    "fitness",
    "management",
    "vue3",
    "typescript"
  ],
  "author": "GymFlow Team",
  "license": "MIT"
}